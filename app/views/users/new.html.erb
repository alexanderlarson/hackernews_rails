<h1>Sign up</h1>

<%= form_for :user, url: users_path do |f| %>
<p>
  <%= f.label :Username %><br>
  <%= f.text_field :username %>
</p>
<p>
  <%= f.label :Email %><br>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label :Password %><br>
  <%= f.text_field :password %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>

